<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:Settings="clr-namespace:EnhancedCombatTracker.Tabs.Settings"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="EnhancedCombatTracker.Tabs.SettingsTab">
  <Grid RowDefinitions="*" ColumnDefinitions="1*,10,4*">
    <Settings:TreeView Grid.Row="0" Grid.Column="0" />
    <GridSplitter Grid.Row="0" Grid.Column="1" Width="10" ></GridSplitter>
    <Panel Grid.Row="0" Grid.Column="2" />
  </Grid>
</UserControl>
